        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Before we draw the camera onto the surfaces, we want to move our camera to the player object.
GMO_followObject(obj_player);

// Get some needed variables
aspect = GMO_getResolutionWidth() / GMO_getResolutionHeight();
znear = GMO_getCameraZNear();
zfar = GMO_getCameraZFar();
prex = x
prey = y
prez = z
// Check if we're viewing by Oculus. If so, we must change the roll, pitch and yaw.

if (GMO_getCameraMode() == 2) { //2 = oculus, 1 = regular.
    //Get our values from the oculus.
    OVR_getEyePos(0);
    OVR_getEyePos(1);
    pitch = OVR_Device_getPitch();
    yaw = OVR_Device_getYaw();
    roll = OVR_Device_getRoll();
    dx = OVR_Device_getY() / 5;
    dy = OVR_Device_getX() / 5 ;
    dz = OVR_Device_getZ() / 5;

    
    x += dx * cos(degtorad(obj_player.direction)) + dy * sin(degtorad(obj_player.direction))
    y += dx * -sin(degtorad(obj_player.direction)) + dy * cos(degtorad(obj_player.direction))
    
    //We want to "add" the head rotation to the body;
    yaw = yaw + obj_player.direction;
    
    z += dz;

    angle = GMO_getCameraAngle(); // The oculus actually needs a wider angle
    
} else { 
    // We're using regular view.
    yaw = obj_player.direction;
    pitch = obj_player.pitch;
    roll = 0;
    
    angle = GMO_getCameraAngle() - 21; //Since the oculus default angle is 111, and the regular cam angle is 90, well - here you go.
}

GMO_renderCamera(x, y, z, yaw, pitch, roll, angle, aspect, znear, zfar); // Renders our image to the surface.

GMO_drawCameraSurface();
x = prex
y = prey
z = prez

</string>
          </argument>
        </arguments>
      </action>
